

#adding elements which are greater than 10
.data

lw t0, 0(zero)   # 10 ->1010
lw t1, 4(zero) #12  ->1100
lw t2, 8(zero)  #1   ->5


add  t3,t1,t0   #22
sub t4,t1,t0     #2
and t5,t1,t0      #1000  ->8
xor t6,t1,t0      #0100 ->4
or s0,t1,t0       #1110 ->14
sll s1,t1,t2     #24
slt  s2,t1,t0    #0
sltu s3,t1,t0    #0
sra  s4,t1,t2    #6
srl  s5,t1,t0    #6
 
 
